{{#if this.hasBuild}}
  <LinkTo @route="ecosystem.repos.repo.build" @models={{array this.build.commit.repo this.build}}>
    <BuildBadge @build={{this.build}} />
  </LinkTo>
  {{#if this.hasFailedBuild}}
    <BuildTree @build={{this.build}} />
  {{/if}}
{{/if}}
<ul class="sonja-dependency-list">
  {{#each @package.requires as |requirement|}}
    <BuildTree::PackageNode @present={{true}} @package={{requirement}} />
  {{/each}}
</ul>